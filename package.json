{
  "name": "taskmanager",
  "version": "1.0.0",
  "description": "Professional monorepo starter for task management with Fastify API, React Web, and Expo Mobile",
  "private": true,
  "scripts": {
    "dev:api": "pnpm --filter api dev",
    "dev:web": "pnpm --filter web dev",
    "dev:mobile": "pnpm --filter mobile start",
    "dev": "pnpm run /^dev:/",
    "build": "pnpm -r build",
    "clean": "pnpm -r exec rm -rf node_modules dist .turbo",
    "db:generate": "pnpm --filter @taskmanager/database exec prisma generate",
    "db:migrate": "pnpm --filter @taskmanager/database exec prisma migrate dev",
    "db:studio": "pnpm --filter @taskmanager/database exec prisma studio"
  },
  "keywords": ["monorepo", "fastify", "expo", "prisma", "better-auth"],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1"
}
